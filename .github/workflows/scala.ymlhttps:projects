name: Scala CI

on:
  push:
    branches: [ doge-rebase-808 ]
  pull_request:
    branches: [ doge-rebase-808 ]

jobs:
  build:

    runs-on: ubuntu-latest

    steps:
    - uses: actions/checkout@v2
    - name: Set up JDK 1.8
      uses: actions/setup-java@v1
      with:
        java-version: 1.8
    - name: Run tests
      run: sbt test
      
      
